          },
          "ownerChangeSerialNumber": {
            "type": "integer",
            "description": "Owner change serial number",
            "format": "int32"
          },
          "coOwnerPersidno": {
            "type": "string",
            "description": "Co onwer social security number",
            "nullable": true
          },
          "coOwnerName": {
