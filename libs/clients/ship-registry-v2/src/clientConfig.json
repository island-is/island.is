  "components": {
    "schemas": {
      "FisheryDto": {
        "required": [
          "COMPANY_ID_NUMBER",
          "CREATION_DATE",
          "ENABLED",
          "FOREIGN",
          "HAND_OFF_DATE",
          "ID",
          "LAST_UPDATE_DATE",
          "SHIP_ID",
          "SOCIAL_SECURITY_NUMBER"
        ],
        "type": "object",
        "properties": {
          "ID": { "type": "integer", "format": "int64" },
          "SOCIAL_SECURITY_NUMBER": { "type": "string" },
          "SHIP_ID": { "type": "integer", "format": "int64" },
          "HAND_OFF_DATE": { "type": "string", "format": "date-time" },
          "COMPANY_ID_NUMBER": { "type": "string" },
          "FOREIGN": { "type": "boolean" },
          "ENABLED": { "type": "boolean" },
          "CREATION_DATE": { "type": "string", "format": "date-time" },
          "LAST_UPDATE_DATE": { "type": "string", "format": "date-time" }
        }
      },
      "ErrorModel": {
        "required": ["description", "parameterName", "traceId"],
        "type": "object",
        "properties": {
          "traceId": { "type": "string" },
          "parameterName": { "type": "string" },
          "description": { "type": "string" }
        }
      },
      "ErrorResponseModel": {
        "required": ["code", "datetime", "description", "errors"],
        "type": "object",
        "properties": {
          "code": { "type": "integer", "format": "int32" },
          "description": { "type": "string" },
          "datetime": { "type": "string", "format": "date-time" },
          "errors": {
            "type": "array",
            "items": { "$ref": "#/components/schemas/ErrorModel" }
          }
        }
      },
      "ShipHAXModel": {
        "required": [
          "bruttoGRT",
          "homeHarbour",
          "hullMaterial",
          "initialRegistrationDate",
          "name",
          "open",
          "region",
          "registrationNumber",
          "status",
          "statusChangeComment",
          "statusChangeDate"
        ],
        "type": "object",
        "properties": {
          "registrationNumber": { "type": "integer", "format": "int64" },
          "name": { "type": "string" },
          "region": { "type": "string" },
          "homeHarbour": { "type": "string" },
          "initialRegistrationDate": {
            "type": "string",
            "format": "date-time"
          },
          "hullMaterial": { "type": "string" },
          "open": { "type": "boolean" },
          "bruttoGRT": { "type": "number", "format": "double" },
          "status": { "type": "string" },
          "statusChangeDate": { "type": "string", "format": "date-time" },
          "statusChangeComment": { "type": "string" }
        }
      },
      "ShipBaseInfoDto": {
        "required": [
          "regionalAcronym",
          "seaWorthyExpiry",
          "shipName",
          "shipRegistrationNumber"
        ],
        "type": "object",
        "properties": {
          "shipName": { "type": "string" },
          "shipRegistrationNumber": { "type": "string" },
          "regionalAcronym": { "type": "string" },
          "seaWorthyExpiry": { "type": "string", "format": "date-time" }
        }
      },
      "CrewMemberModel": {
        "required": ["crewRegistrationNumber", "rank", "sailor"],
        "type": "object",
        "properties": {
          "crewRegistrationNumber": { "type": "integer", "format": "int64" },
          "rank": { "$ref": "#/components/schemas/RankModel" },
          "sailor": { "$ref": "#/components/schemas/SailorModel" }
        }
      },
      "EngineModel": {
        "required": [
          "manufacturer",
          "manufacturingNumber",
          "power",
          "usage",
          "year"
        ],
