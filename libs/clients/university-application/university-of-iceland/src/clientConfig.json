    "/program/{externalId}/courses": {
      "get": {
        "summary": "Get courses for a program",
        "parameters": [
          {
            "name": "externalId",
            "required": true,
            "in": "path",
            "allowEmptyValue": false,
            "description": "Application ID",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "externalId": {
                            "type": "array",
                            "items": {
                              "type": "string"
                            }
                          },
                          "nameIs": {
                            "type": "string"
                          },
                          "nameEn": {
                            "type": "string"
                          },
                          "required": {
                            "type": "string",
                            "enum": [
                              "MANDATORY",
                              "ELECTIVE",
                              "RESTRICTED_ELECTIVE"
                            ]
                          },
                          "credits": {
                            "type": "string"
                          },
                          "semesterYear": {
                            "type": "string"
                          },
                          "semesterSeason": {
                            "type": "string",
                            "enum": [
                              "SPRING",
                              "FALL",
                              "SUMMER",
                              "WHOLE-YEAR",
                              "ANY"
                            ]
                          },
                          "descriptionIs": {
                            "type": "string"
                          },
                          "descriptionEn": {
                            "type": "string"
                          },
                          "externalUrlIs": {
                            "type": "string"
                          },
                          "externalUrlEn": {
                            "type": "string"
                          },
                          "displayId": {
                            "type": "string"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "tags": ["Courses"]
      }
    },
