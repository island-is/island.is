{
  "root": "libs/clients/icelandic-government-institution-vacancies",
  "sourceRoot": "libs/clients/icelandic-government-institution-vacancies/src",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/clients/icelandic-government-institution-vacancies/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/clients/icelandic-government-institution-vacancies"
      ],
      "options": {
        "jestConfig": "libs/clients/icelandic-government-institution-vacancies/jest.config.js",
        "passWithNoTests": true
      }
    },
    "update-openapi-document": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "curl -H \"X-Road-Client: IS-DEV/GOV/10000/island-is-client\" http://localhost:8081/r1/IS-DEV/GOV/10021/FJS-Protected/getOpenAPI?serviceCode=recruitment-v1 > src/clientConfig.yml",
          "prettier --write src/clientConfig.yml"
        ],
        "parallel": false,
        "cwd": "libs/clients/icelandic-government-institution-vacancies"
      }
    },
    "schemas/external-openapi-generator": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "yarn openapi-generator -o libs/clients/icelandic-government-institution-vacancies/gen/fetch -i libs/clients/icelandic-government-institution-vacancies/src/clientConfig.yml"
      }
    }
  },
  "tags": ["lib:client"]
}
