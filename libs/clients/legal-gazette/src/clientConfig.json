          "type": { "$ref": "#/components/schemas/CaseTypeDetailedDto" }
        },
        "required": [
          "id",
          "title",
          "slug",
          "createdAt",
          "updatedAt",
          "typeId",
          "type"
        ]
      },
      "GetCategoriesDetailedDto": {
        "type": "object",
        "properties": {
          "categories": {
            "type": "array",
            "items": { "$ref": "#/components/schemas/CategoryDetailedDto" }
          }
