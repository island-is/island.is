          }
        },
        "required": ["types"]
      },
      "ApiErrorDto": {
        "type": "object",
        "properties": {
          "statusCode": { "type": "number" },
          "timestamp": { "type": "string" },
          "name": {
            "type": "string",
            "enum": [
              "NotFound",
              "ValidationError",
              "ForeignKeyConstraintError",
              "UniqueConstraintError",
              "TimeoutError",
              "Unauthorized",
              "Forbidden",
              "BadRequest",
              "InternalServerError",
              "UnknownError"
            ],
            "example": [
              "NotFound",
              "ValidationError",
              "ForeignKeyConstraintError",
              "UniqueConstraintError",
              "TimeoutError",
              "Unauthorized",
              "Forbidden",
              "BadRequest",
              "InternalServerError",
              "UnknownError"
            ]
          },
          "message": { "type": "string" },
          "details": { "type": "array", "items": { "type": "string" } }
        },
        "required": ["statusCode", "timestamp"]
      },
      "CreateCaseTypeDto": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "description": "Title of the entity",
            "example": "Base entity"
          }
        },
        "required": ["title"]
      },
      "GetCaseTypeDto": {
        "type": "object",
        "properties": {
          "type": { "$ref": "#/components/schemas/CaseTypeDto" }
        },
        "required": ["type"]
      },
      "UpdateCaseTypeDto": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "description": "Title of the entity",
            "example": "Base entity"
          }
        }
      },
      "ApplicationType": { "type": "string", "enum": ["almenn-auglysing"] },
      "CategoryDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "UUIDV4 of the entity",
            "example": "182391f7-d355-46f0-bcee-4d8baa68b55d"
          },
          "title": {
            "type": "string",
            "description": "Title of the entity",
            "example": "Base entity"
          },
          "slug": {
            "type": "string",
            "description": "Slug of the entity",
            "example": "base-entity"
          },
          "typeId": { "type": "string" },
          "type": { "$ref": "#/components/schemas/CaseTypeDto" }
        },
        "required": ["id", "title", "slug", "typeId", "type"]
      },
      "GetCategoriesDto": {
        "type": "object",
        "properties": {
          "categories": {
            "type": "array",
