        },
        "required": ["types"]
      },
      "ApiErrorDto": {
        "type": "object",
        "properties": {
          "statusCode": { "type": "number" },
          "timestamp": { "type": "string" },
          "name": {
            "type": "string",
            "enum": [
              "NotFound",
              "ValidationError",
              "ForeignKeyConstraintError",
              "UniqueConstraintError",
              "TimeoutError",
              "Unauthorized",
              "Forbidden",
              "BadRequest",
              "InternalServerError",
              "UnknownError"
            ],
            "example": [
              "NotFound",
              "ValidationError",
              "ForeignKeyConstraintError",
              "UniqueConstraintError",
              "TimeoutError",
              "Unauthorized",
              "Forbidden",
              "BadRequest",
              "InternalServerError",
              "UnknownError"
            ]
          },
          "message": { "type": "string" },
          "details": { "type": "string" }
        },
        "required": ["statusCode", "timestamp"]
      },
      "CreateCaseTypeDto": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "description": "Title of the entity",
            "example": "Base entity"
          }
        },
        "required": ["title"]
      },
      "GetCaseTypeDto": {
        "type": "object",
        "properties": {
          "type": { "$ref": "#/components/schemas/CaseTypeDto" }
        },
        "required": ["type"]
      },
