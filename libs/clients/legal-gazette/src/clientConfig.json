      "SubmitApplicationDto": {
        "type": "object",
        "properties": {
          "applicationId": { "type": "string" },
          "categoryId": { "type": "string" },
          "caption": { "type": "string" },
          "htmlBase64": { "type": "string" },
          "signature": { "$ref": "#/components/schemas/SignatureDto" },
          "channels": {
            "type": "array",
