  "components": {
    "schemas": {
      "CaseTypeDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "UUIDV4 of the entity",
            "example": "182391f7-d355-46f0-bcee-4d8baa68b55d"
          },
          "title": {
            "type": "string",
            "description": "Title of the entity",
            "example": "Base entity"
          },
          "slug": {
            "type": "string",
            "description": "Slug of the entity",
            "example": "base-entity"
          }
        },
        "required": ["id", "title", "slug"]
      },
      "GetCaseTypesDto": {
        "type": "object",
        "properties": {
          "types": {
