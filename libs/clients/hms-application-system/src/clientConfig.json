          },
          "language": {
            "type": "string",
            "nullable": true
          },
          "portalApplicationID": {
            "type": "string",
            "nullable": true
          },
          "applicationType": {
            "type": "string",
            "nullable": true
          },
          "dagssetning": {
            "type": "string",
            "format": "date-time"
          },
          "externalId": {
            "type": "string",
            "nullable": true
          },
          "applicantName": {
            "type": "string",
            "nullable": true
          },
          "applicantKennitala": {
            "type": "string",
            "nullable": true
          },
