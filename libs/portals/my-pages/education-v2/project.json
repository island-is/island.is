{
  "name": "portals-my-pages-education-v2",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/portals/my-pages/education-v2/src",
  "projectType": "library",
  "tags": ["lib:portals-mypages", "scope:portals-mypages"],
  "generators": {},
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "libs/portals/my-pages/education-v2/jest.config.ts"
      },
      "outputs": ["{workspaceRoot}/coverage/libs/portals/my-pages/education-v2"]
    },
    "extract-strings": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn ts-node -P libs/localization/tsconfig.lib.json libs/localization/scripts/extract 'libs/portals/my-pages/education-v2/src/{lib,components,screens}/**/*.{js,ts,tsx}'"
      }
    },
    "codegen/frontend-client": {
      "executor": "nx:run-commands",
      "options": {
        "output": "libs/portals/my-pages/education-v2/src/**/*.generated.ts",
        "command": "graphql-codegen --config libs/portals/my-pages/education-v2/codegen.yml"
      }
    }
  }
}
