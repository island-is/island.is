{
  "name": "application-templates-id-card",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/application/templates/id-card/src",
  "projectType": "library",
  "tags": ["scope:application-system", "lib:application-system"],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/application/templates/id-card/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/application/templates/id-card/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "extract-strings": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn ts-node -P libs/localization/tsconfig.lib.json libs/localization/scripts/extract 'libs/application/templates/id-card/src/lib/messages/*.ts'"
      }
    },
    "configurations": {
      "ci": {
        "ci": true,
        "codeCoverage": true
      }
    }
  }
}
