{
  "name": "application-templates-mile-car",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/application/templates/transport-authority/mile-car/src",
  "projectType": "library",
  "tags": ["scope:application-system", "lib:application-system"],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint"
