{
  "root": "apps/system-e2e",
  "sourceRoot": "apps/system-e2e/src",
  "projectType": "application",
  "prefix": "system-e2e",
  "generators": {},
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "esbuild --bundle $(find apps/system-e2e -name '*.spec.ts') --outdir=dist/apps/system-e2e --tsconfig=apps/system-e2e/tsconfig.json --platform=node $(jq -r '.dependencies|keys[]' package.json | xargs -I{} echo \"--external:{} \") --external:./node_modules/*",
          "echo cp package.json dist/apps/system-e2e/"
        ],
        "parallel": false,
        "outputPath": "dist/apps/system-e2e"
      },
      "configurations": {
        "production": {}
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/system-e2e/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "docker-playwright": {}
  },
  "tags": ["scope:e2e"]
}
