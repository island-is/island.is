{
  "name": "@island.is/native-app",
  "version": "1.2.5",
  "license": "MIT",
  "scripts": {
    "release:android": "cd android && ./gradlew bundleProdRelease bundleDevRelease",
    "android": "react-native run-android --mode devDebug",
    "android:prod": "react-native run-android --variant=prodDebug",
    "ios": "react-native run-ios --scheme Ísland.dev",
    "ios:prod": "react-native run-ios --scheme Ísland.is",
    "build-mock": "echo FIXME",
    "start": "react-native start",
    "start:debug": "yarn start --experimental-debugger",
    "fastlane:ios": "cd ios && fastlane",
    "fastlane:android": "cd android && fastlane",
    "beta": "yarn prerequisites && yarn fastlane:ios beta && yarn fastlane:android beta",
    "alpha:upload": "yarn prerequisites && yarn fastlane:ios beta apps:dev && yarn fastlane:android beta apps:dev",
    "alpha:release": "yarn prerequisites && yarn fastlane:ios promote to:alpha apps:dev && yarn fastlane:android promote to:alpha apps:dev",
    "codepush:ios": "yarn prerequisites && appcenter codepush release-react -a Stafraent-Island/Island-ios -x -d production  --plist-file ios/IslandApp/Info.plist",
    "codepush:android": "yarn prerequisites && appcenter codepush release-react -a Stafraent-Island/Island-android -x -d production",
    "test": "jest",
    "test-mock": "echo FIXME",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook",
    "nx:typecheck": "tsc --noEmit -p tsconfig.app.json",
    "nx:codegen": "yarn run -T nx run native-app:codegen/frontend-client",
    "nx:lint": "yarn run -T nx run native-app:lint",
    "nx:build": "yarn run -T nx run native-app:build",
    "prerequisites": "yarn nx:codegen && yarn nx:typecheck && yarn nx:lint"
  },
