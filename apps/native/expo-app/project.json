{
  "name": "expo-app",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/native/expo-app/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "start": {
      "executor": "@nx/expo:start",
      "dependsOn": ["sync-deps"],
      "options": {}
    },
    "serve": {
      "executor": "@nx/expo:serve",
      "dependsOn": ["sync-deps"],
      "options": {
        "port": 4200
      }
    },
    "run-ios": {
      "executor": "@nx/expo:run",
      "dependsOn": ["sync-deps"],
      "options": {
        "platform": "ios"
      }
    },
    "run-android": {
      "executor": "@nx/expo:run",
      "dependsOn": ["sync-deps"],
      "options": {
        "platform": "android"
      }
    },
    "build": {
      "executor": "@nx/expo:build",
      "dependsOn": ["sync-deps"],
      "options": {}
    },
    "submit": {
      "executor": "@nx/expo:submit",
      "options": {}
    },
    "build-list": {
      "executor": "@nx/expo:build-list",
      "options": {}
    },
    "sync-deps": {
      "executor": "@nx/expo:sync-deps",
      "options": {}
    },
    "prebuild": {
      "executor": "@nx/expo:prebuild",
      "dependsOn": ["sync-deps"],
      "options": {}
    },
    "install": {
      "executor": "@nx/expo:install",
      "options": {}
    },
    "update": {
      "executor": "@nx/expo:update",
      "options": {}
    },
    "export": {
      "executor": "@nx/expo:export",
      "dependsOn": ["sync-deps"],
      "outputs": ["{options.outputDir}"],
      "options": {
        "platform": "all",
        "outputDir": "dist/apps/native/expo-app"
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "dependsOn": ["export"],
      "options": {
        "buildTarget": "expo-app:export",
        "staticFilePath": "dist/apps/native/expo-app",
        "spa": true
      }
    }
  }
}
